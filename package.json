{
  "name": "gh-node-cli",
  "version": "1.0.0",
  "description": "CLI to fetch the GitHub and then list the fetched users from a local database.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "tsc && node ./dist/main.js",
    "list": "node ./dist/main.js",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "create-db": "db-migrate db:create lovelystay",
    "drop-db": "db-migrate db:drop lovelystay",
    "create-migration": "db-migrate create",
    "migrate-up": "db-migrate up",
    "migrate-down": "db-migrate down"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.0.4",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "minimist": "^1.2.6",
    "pg-promise": "^10.11.1",
    "ramda": "^0.28.0",
    "yargs": "^15.4.1"
  }
}
